	function fnAttachToggle(bl){
		var Obj = document.getElementById("vfile");
		
		if(bl){
			Obj.style.visibility  = "visible";
		}else{
			Obj.style.visibility  = "hidden";
		}
	}
	
	function fnSelect(val){
		var pstate = "<%=pstate%>";
		if(val == "제정" && pstate !='U'){
			fnAttachToggle(true);
		}else if(val == "개정" ||val == "폐지"){
			fnAttachToggle(false);
		}else if(val == "전부개정"){
			fnAttachToggle(true);
		}else{
			fnAttachToggle(false);
		}
	}
	function goChange(obj){
		if(obj.checked){
			fnAttachToggle(true);
		}else{
			fnAttachToggle(false);
		}
	}

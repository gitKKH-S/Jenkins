	var extArray = new Array("hwp","gul");
	function LimitAttach(form,file) {
	 	file = file.split('.');
		var allowSubmit = false;
		if (!file) return;

		for (var i = 0; i < extArray.length; i++) {
			if (extArray[i] == file[file.length-1]) {
				allowSubmit = true;
				break;
			}
		}

		if(!allowSubmit){
			alert("죄송합니다.업로드가 지원되는 파일형식은 "
					+ "한글 또는 훈민정음 파일 입니다."
					+ "파일형식을 확인해보시기 바랍니다.");
			dell(form.name);
			return;
		}

		var frm = form = document.frmWrite;
		frm.action = "<%=CONTEXTPATH%>/bylaw/adm/bylawEnactmentFileUpload.do";
		$('#frmWrite').ajaxForm( {
		    dataType : "json",
		    error : function() {
		        alert("처리중 오류가 발생하였습니다.");
		    },
		    success : function(data){
		    	form = document.frmWrite;
				if(form.TITLE.value==''){
					file = file[0].split('\\');
					form.TITLE.value = file[file.length-1];
				}
				form.FILENAME.value=data.sfName;
				form.encoding = "application/x-www-form-urlencoded"; 
				form.action="<%=CONTEXTPATH%>/bylaw/adm/bylawEnactmentHwpedit.do";
				if(form.TITLE.value==''){
					alert("제목이 입력되지 않았습니다.");
					return;
				}
				if(form.DEPTNAME.value==''){
					alert("담당부서를 선택해야 합니다.");
					//return;
				}
				if(form.CHG.checked){
					form.CHG.value='Y';
				}else{
					form.CHG.value='N';
				}
				if(form.BUYN.checked){
					form.BUYN.value='Y';
				}else{
					form.BUYN.value='N';
				}
				if(form.ACTIONYN.checked){
					form.ACTIONYN.value='Y';
				}else{
					form.ACTIONYN.value='N';
				}
				if(form.HOMEYN.checked){
					form.HOMEYN.value='Y';
				}else{
					form.HOMEYN.value='N';
				}
				if(form.BUCHKYN.checked){
					form.BUCHKYN.value='Y';
				}else{
					form.BUCHKYN.value='N';
				}
				if(form.PRELAWYN.checked){
					form.PRELAWYN.value='Y';
				}else{
					form.PRELAWYN.value='N';
				}
				if(form.OPENYN.checked){
					form.OPENYN.value='Y';
				}else{
					form.OPENYN.value='N';
				}
				form.method="post";
				form.submit();
		    }
		});
		$('#frmWrite').submit();
	}
	function dell(key){
	  var fileList = document.getElementsByTagName("input");
	  for (var i=0; i < fileList.length ;i++ ){
	    if(fileList[i].type == "file" && fileList[i].name==key){
	      fileList[i].select();
	      document.selection.clear();
	    }
	  }
	}
	function viewFile2(key){
	   if(key=='개정'||key=='폐지'){
			document.all.vfile.style.display='none';
		}else{
			document.all.vfile.style.display='';
		}
	}
	function goWrite(){
		frm = document.forms[0];
		if(frm.TITLE.value==''){
			alert('규정명을 입력하셔야 합니다.');
			return;
		}
		if("<%=pstate%>"!='U' && (frm.REVCD.value == '제정' || frm.REVCD.value == '전부개정')){
			LimitAttach(frm,frm.bylawFile.value);
		}else{
			if(frm.CHG.checked){
				frm.CHG.value='Y';
			}else{
				frm.CHG.value='N';
			}
			if(frm.BUYN.checked){
				frm.BUYN.value='Y';
			}else{
				frm.BUYN.value='N';
			}
			if(frm.ACTIONYN.checked){
				frm.ACTIONYN.value='Y';
			}else{
				frm.ACTIONYN.value='N';
			}
			if(frm.HOMEYN.checked){
				frm.HOMEYN.value='Y';
			}else{
				frm.HOMEYN.value='N';
			}
			if(frm.BUCHKYN.checked){
				frm.BUCHKYN.value='Y';
			}else{
				frm.BUCHKYN.value='N';
			}
			if(frm.PRELAWYN.checked){
				frm.PRELAWYN.value='Y';
			}else{
				frm.PRELAWYN.value='N';
			}
			if(frm.OPENYN.checked){
				frm.OPENYN.value='Y';
			}else{
				frm.OPENYN.value='N';
			}
			parent.msgbox();
			frm.submit();
		}
	}
	function getUserList(key){
 		//alert(key);
		var v = key.split("/");
		frm = document.frmWrite;
		frm.DEPTNAME.value = v[1];
		frm.DEPT.value = v[0];
		frm.USERID.value = "";
		frm.USERNAME.value = "";
		frm.PHONE.value = "";
		ajaxuserInfo(v[0]);
	}
	
	function ajaxuserInfo(deptcd){
		if(deptcd!=''){
			$.ajax({
				type:'post',
				url:CONTEXTPATH+'/bylaw/adm/getUserList.do',
				dataType: "json",
				data : {
					Deptcd : deptcd
				},
				async: false,
				success:function(data){
					getRe(data);
				},
				error:function(e){
					alert("사용자 정보가 존재 하지 않습니다.");
				}
			})
		}
	}
	function getRe(jdata){
		var userid = "<%=meta.get("USERID")==null?"":meta.get("USERID") %>";
		frm = document.frmWrite;
		var selectObj = frm.userSel;
		$('#userSel').empty(); 
		var option = new Option();
		option.value = "";
		option.text = "--담당자 선택--";
		selectObj.options.add(option);
		for(k=0; k<jdata.data.length; k++){
			option = new Option();
			option.value = jdata.data[k].PHONE+"/"+jdata.data[k].USERID+"/"+jdata.data[k].USERNAME;
			option.text = jdata.data[k].USERNAME;
			if(userid == jdata.data[k].USERID){
				option.selected = true;
			}
			selectObj.options.add(option);
			//if(jdata[k].grade=='44'){
				//frm.approver.value=jdata[k].username;
				//frm.approverid.value=jdata[k].userid;
			//}
		}
	}
	function userInfo(key){
		var v = key.split("/");
		frm = document.frmWrite;
		frm.USERID.value = v[1];
		frm.USERNAME.value = v[2];
		frm.PHONE.value = v[0]=="null"?"":v[0];
	}
	
